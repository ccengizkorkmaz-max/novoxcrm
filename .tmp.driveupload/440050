    {"offset": {"line": 2831, "column": 0}, "map": {"version":3,"sources":["file:///C:/NOVOCRM/src/components/back-button.tsx"],"sourcesContent":["'use client'\r\n\r\nimport { useRouter } from 'next/navigation'\r\nimport { Button } from '@/components/ui/button'\r\nimport { ArrowLeft, ChevronLeft } from 'lucide-react'\r\nimport { cn } from '@/lib/utils'\r\n\r\ninterface BackButtonProps {\r\n    href?: string\r\n    label?: string\r\n    className?: string\r\n    variant?: \"link\" | \"default\" | \"destructive\" | \"outline\" | \"secondary\" | \"ghost\"\r\n    size?: \"default\" | \"sm\" | \"lg\" | \"icon\"\r\n    iconType?: \"chevron\" | \"arrow\"\r\n}\r\n\r\nexport function BackButton({\r\n    href,\r\n    label,\r\n    className,\r\n    variant = \"ghost\",\r\n    size = \"sm\",\r\n    iconType = \"arrow\"\r\n}: BackButtonProps) {\r\n    const router = useRouter()\r\n\r\n    const handleBack = () => {\r\n        // Preference: router.back() to keep filters/state\r\n        // If href is provided, it can be used as a fallback or explicit override \r\n        // (but user requested \"previous page\")\r\n        if (typeof window !== 'undefined' && window.history.length > 2) {\r\n            router.back()\r\n        } else if (href) {\r\n            router.push(href)\r\n        } else {\r\n            router.back()\r\n        }\r\n    }\r\n\r\n    const Icon = iconType === \"chevron\" ? ChevronLeft : ArrowLeft\r\n\r\n    return (\r\n        <Button\r\n            variant={variant}\r\n            size={size}\r\n            onClick={handleBack}\r\n            className={cn(\"gap-2\", className)}\r\n        >\r\n            <Icon className={cn(\"h-4 w-4\", size === \"icon\" && \"h-5 w-5\")} />\r\n            {label && <span>{label}</span>}\r\n            {!label && size !== \"icon\" && <span>Geri Dön</span>}\r\n        </Button>\r\n    )\r\n}\r\n"],"names":[],"mappings":";;;;;AAEA;AACA;AACA;AAAA;AACA;AALA;;;;;;AAgBO,SAAS,WAAW,EACvB,IAAI,EACJ,KAAK,EACL,SAAS,EACT,UAAU,OAA