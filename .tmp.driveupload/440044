        onValueChange?.(validRooms);
    }, [
        rooms,
        onValueChange
    ]);
    const addRoom = ()=>{
        setRooms((prev)=>[
                ...prev,
                {
                    room: '',
                    area: 0
                }
            ]);
    };
    const removeRoom = (index)=>{
        setRooms((prev)=>prev.filter((_, i)=>i !== index));
    };
    const updateRoom = (index, field, value)=>{
        setRooms((prev)=>{
            const newRooms = [
                ...prev
            ];
            newRooms[index] = {
                ...newRooms[index],
                [field]: value
            };
            return newRooms;
        });
    };
