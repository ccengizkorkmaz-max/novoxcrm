window.open(`https://www.google.com/maps/search/${encodeURIComponent(searchQuery)}`, '_blank')\r\n    }\r\n\r\n    const handleViewLocation = () => {\r\n        if (latitude && longitude) {\r\n            window.open(`https://www.google.com/maps?q=${latitude},${longitude}`, '_blank')\r\n        } else if (address) {\r\n            window.open(`https://www.google.com/maps/search/${encodeURIComponent(address)}`, '_blank')\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"flex gap-2\">\r\n            <Input\r\n                name=\"location_display\"\r\n                defaultValue={address || ''}\r\n                placeholder=\"Lokasyon se√ßilmedi\"\r\n                className=\"flex-1\"\r\n                readOnly\r\n            />\r\n            <input type=\"hidden\" name=\"latitude\" defaultValue={latitude || ''} />\r\n            <input type=\"hidden\" name=\"longitude\" defaultValue={longitude || ''} />\r\n\r\n            <Dialog open={isOpen} onOpenChange={setIsOpen}>\r\n                <DialogTrigger asChild>\r\n                    <Button type=\"button\" variant=\"outline\" size=\"icon\" title=\"Lokasyon D√ºzenle\">\r\n                        <MapPin className=\"h-4 w-4\" />\r\n                    </Button>\r\n                </DialogTrigger>\r\n                <DialogContent className=\"sm:max-w-[500px]\">\r\n                    <DialogHeader>\r\n                        <DialogTitle>Lokasyon Bilgisi</DialogTitle>\r\n                        <DialogDescription>\r\n                            Google Maps'ten koordinatlarƒ± kopyalayƒ±p buraya yapƒ±≈ütƒ±rƒ±n.\r\n                        </DialogDescription>\r\n                    </DialogHeader>\r\n                    <div className=\"space-y-4 py-4\">\r\n                        <div className=\"space-y-2\">\r\n                            <Label htmlFor=\"temp-address\">Adres</Label>\r\n                            <Input\r\n                                id=\"temp-address\"\r\n                                value={tempAddress}\r\n                                onChange={(e) => setTempAddress(e.target.value)}\r\n                                placeholder=\"√ñrn: Fatih, ƒ∞stanbul\"\r\n                            />\r\n                        </div>\r\n                        <div className=\"grid grid-cols-2 gap-4\">\r\n                            <div className=\"space-y-2\">\r\n                                <Label htmlFor=\"temp-lat\">Enlem (Latitude)</Label>\r\n                                <Input\r\n                                    id=\"temp-lat\"\r\n                                    type=\"number\"\r\n                                    step=\"any\"\r\n                                    value={tempLat}\r\n                                    onChange={(e) => setTempLat(e.target.value)}\r\n                                    placeholder=\"41.0082\"\r\n                                />\r\n                            </div>\r\n                            <div className=\"space-y-2\">\r\n                                <Label htmlFor=\"temp-lng\">Boylam (Longitude)</Label>\r\n                                <Input\r\n                                    id=\"temp-lng\"\r\n                                    type=\"number\"\r\n                                    step=\"any\"\r\n                                    value={tempLng}\r\n                                    onChange={(e) => setTempLng(e.target.value)}\r\n                                    placeholder=\"28.9784\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <Button\r\n                            type=\"button\"\r\n                            variant=\"outline\"\r\n                            className=\"w-full\"\r\n                            onClick={handleOpenMaps}\r\n                        >\r\n                            <ExternalLink className=\"mr-2 h-4 w-4\" />\r\n                            Google Maps'te A√ß\r\n                        </Button>\r\n                        <p className=\"text-xs text-muted-foreground\">\r\n                            Google Maps'te konumu bulun, koordinatlara saƒü tƒ±klayƒ±p kopyalayƒ±n ve yukarƒ±daki alanlara yapƒ±≈ütƒ±rƒ±n.\r\n                        </p>\r\n                    </div>\r\n                    <DialogFooter>\r\n                        <Button\r\n                            type=\"button\"\r\n                            variant=\"outline\"\r\n                            onClick={() => setIsOpen(false)}\r\n                        >\r\n                            ƒ∞ptal\r\n                        </Button>\r\n                        <Button\r\n                            type=\"button\"\r\n                            onClick={handleSave}\r\n                        >\r\n                            Kaydet\r\n                        </Button>\r\n                    </DialogFooter>\r\n                </DialogContent>\r\n            </Dialog>\r\n\r\n            {(latitude && longitude) && (\r\n                <Button\r\n                    type=\"button\"\r\n                    variant=\"outline\"\r\n                    size=\"sm\"\r\n                    onClick={handleViewLocation}\r\n                    title=\"Google Maps'te G√∂ster\"\r\n                >\r\n                    Haritada G√∂ster\r\n                </Button>\r\n            )}\r\n        </div>\r\n    )\r\n}\r\n","'use client'\r\n\r\nimport { useState } from 'react'\r\nimport { Button } from '@/components/ui/button'\r\nimport { Input } from '@/components/ui/input'\r\nimport { Label } from '@/components/ui/label'\r\nimport { Textarea } from '@/components/ui/textarea'\r\nimport { Upload, Loader2 } from 'lucide-react'\r\nimport {\r\n    Dialog,\r\n    DialogContent,\r\n    DialogDescription,\r\n    DialogFooter,\r\n    DialogHeader,\r\n    DialogTitle,\r\n    DialogTrigger,\r\n} from \"@/components/ui/dialog\"\r\n\r\ninterface DocumentUploadProps {\r\n    projectId: string\r\n    onUpload: (formData: FormData) => Promise<any>\r\n}\r\n\r\nexport function DocumentUpload({ projectId, onUpload }: DocumentUploadProps) {\r\n    const [open, setOpen] = useState(false)\r\n    const [loading, setLoading] = useState(false)\r\n    const [selectedFile, setSelectedFile] = useState<File | null>(null)\r\n\r\n    async function handleSubmit(e: React.FormEvent<HTMLFormElement>) {\r\n        e.preventDefault()\r\n        setLoading(true)\r\n\r\n        const formData = new FormData(e.currentTarget)\r\n        formData.append('project_id', projectId)\r\n\r\n        try {\r\n            const result = await onUpload(formData)\r\n\r\n            setLoading(false)\r\n\r\n            if (result?.success) {\r\n                alert('D√∂k√ºman ba≈üarƒ±yla y√ºklendi!')\r\n                setOpen(false)\r\n                setSelectedFile(null)\r\n                    ; (e.target as HTMLFormElement).reset()\r\n                // Force page reload to show new document\r\n                window.location.reload()\r\n            } else {\r\n                alert(result?.error || 'Y√ºkleme ba≈üarƒ±sƒ±z. L√ºtfen tekrar deneyin.')\r\n            }\r\n        } catch (error) {\r\n            setLoading(false)\r\n            console.error('Upload error:', error)\r\n            alert('Bir hata olu≈ütu. L√ºtfen konsolu kontrol edin.')\r\n        }\r\n    }\r\n\r\n    return (\r\n        <Dialog open={open} onOpenChange={setOpen}>\r\n            <DialogTrigger asChild>\r\n                <Button>\r\n                    <Upload className=\"mr-2 h-4 w-4\" />\r\n                    D√∂k√ºman Y√ºkle\r\n                </Button>\r\n            </DialogTrigger>\r\n            <DialogContent className=\"sm:max-w-[500px]\">\r\n                <DialogHeader>\r\n                    <DialogTitle>Yeni D√∂k√ºman Y√ºkle</DialogTitle>\r\n                    <DialogDescription>\r\n                        Projeye ait d√∂k√ºman y√ºkleyin (PDF, resim, Word, Excel vb.)\r\n                    </DialogDescription>\r\n                </DialogHeader>\r\n                <form onSubmit={handleSubmit}>\r\n                    <div className=\"grid gap-4 py-4\">\r\n                        <div className=\"space-y-2\">\r\n                            <Label htmlFor=\"document_name\">D√∂k√ºman Adƒ± *</Label>\r\n                            <Input\r\n                                id=\"document_name\"\r\n                                name=\"document_name\"\r\n                                placeholder=\"√ñrn: ƒ∞mar Planƒ±\"\r\n                                required\r\n                            />\r\n                        </div>\r\n                        <div className=\"space-y-2\">\r\n                            <Label htmlFor=\"description\">A√ßƒ±klama</Label>\r\n                            <Textarea\r\n                                id=\"description\"\r\n                                name=\"description\"\r\n                                placeholder=\"D√∂k√ºman hakkƒ±nda kƒ±sa a√ßƒ±klama\"\r\n                                rows={3}\r\n                            />\r\n                        </div>\r\n                        <div className=\"space-y-2\">\r\n                            <Label htmlFor=\"file\">Dosya *</Label>\r\n                            <Input\r\n                                id=\"file\"\r\n                                name=\"file\"\r\n                                type=\"file\"\r\n                                required\r\n                                onChange={(e) => setSelectedFile(e.target.files?.[0] || null)}\r\n                            />\r\n                            {selectedFile && (\r\n                                <p className=\"text-xs text-muted-foreground\">\r\n                                    Se√ßili: {selectedFile.name} ({(selectedFile.size / 1024 / 1024).toFixed(2)} MB)\r\n                                </p>\r\n                            )}\r\n                        </div>\r\n                    </div>\r\n                    <DialogFooter>\r\n                        <Button\r\n                            type=\"button\"\r\n                            variant=\"outline\"\r\n                            onClick={() => setOpen(false)}\r\n                            disabled={loading}\r\n                        >\r\n                            ƒ∞ptal\r\n                        </Button>\r\n                        <Button type=\"submit\" disabled={loading}>\r\n                            {loading && <Loader2 className=\"mr-2 h-4 w-4 animate-spin\" />}\r\n                            Y√ºkle\r\n                        </Button>\r\n                    </DialogFooter>\r\n                </form>\r\n            </DialogContent>\r\n        </Dialog>\r\n    )\r\n}\r\n","'use client'\r\n\r\nimport { useState } from 'react'\r\nimport { Button } from \"@/components/ui/button\"\r\nimport {\r\n    Dialog,\r\n    DialogContent,\r\n    DialogDescription,\r\n    DialogFooter,\r\n    DialogHeader,\r\n    DialogTitle,\r\n    DialogTrigger,\r\n} from \"@/components/ui/dialog\"\r\nimport { Input } from \"@/components/ui/input\"\r\nimport { Label } from \"@/components/ui/label\"\r\nimport { Plus } from \"lucide-react\"\r\nimport { RoomAreasInput } from \"@/components/room-areas-input\"\r\n\r\nimport { useRouter } from 'next/navigation'\r\n\r\ninterface BatchUnitCreatorProps {\r\n    projectId: string\r\n    action: (formData: FormData) => Promise<any>\r\n}\r\n\r\nconst UNIT_CATEGORIES = [\r\n    \"Daire\", \"Depo\", \"D√ºkkan\", \"Ofis\", \"Villa\",\r\n    \"Dubleks Daire\", \"Bah√ße Dubleks Daire\", \"√áatƒ± Dubleks Daire\",\r\n    \"Roof Daire\", \"Loft Daire\", \"Penthouse\", \"Ticari Alan\"\r\n]\r\n\r\nexport function BatchUnitCreator({ projectId, action }: BatchUnitCreatorProps) {\r\n    const [open, setOpen] = useState(false)\r\n    const [loading, setLoading] = useState(false)\r\n    const [error, setError] = useState<string | null>(null)\r\n    const [roomAreas, setRoomAreas] = useState<any[]>([])\r\n    const router = useRouter()\r\n\r\n    async function handleSubmit(formData: FormData) {\r\n        setLoading(true)\r\n        setError(null)\r\n\r\n        // Manually append room_areas if not present or empty\r\n        if (roomAreas.length > 0) {\r\n            formData.set('room_areas', JSON.stringify(roomAreas))\r\n        }\r\n\r\n        console.log('BatchUnitCreator room_areas (manual):', formData.get('room_areas'))\r\n\r\n        const result = await action(formData)\r\n        setLoading(false)\r\n\r\n        if (result?.error) {\r\n            console.error('Batch Create Error:', result.error)\r\n            setError(result.error)\r\n            return\r\n        }\r\n\r\n        setOpen(false)\r\n        router.refresh()\r\n        // Ensure we stay on the units tab and refresh logic works\r\n        router.push(`/projects/${projectId}?tab=units`)\r\n    }\r\n\r\n    return (\r\n        <Dialog open={open} onOpenChange={setOpen}>\r\n            <DialogTrigger asChild>\r\n                <Button>\r\n                    <Plus className=\"w-4 h-4 mr-2\" /> √únite Ekle\r\n                </Button>\r\n            </DialogTrigger>\r\n            <DialogContent className=\"sm:max-w-[800px]\">\r\n                <DialogHeader>\r\n                    <DialogTitle>Toplu √únite Olu≈üturma</DialogTitle>\r\n                    <DialogDescription>\r\n                        Birden fazla √ºniteyi hƒ±zlƒ±ca sisteme ekleyin.\r\n                    </DialogDescription>\r\n                </DialogHeader>\r\n                <form action={handleSubmit}>\r\n                    <input type=\"hidden\" name=\"project_id\" value={projectId} />\r\n\r\n                    <div className=\"py-4 space-y-6\">\r\n                        {/* Temel Bilgiler */}\r\n                        <div className=\"space-y-4\">\r\n                            <h3 className=\"text-sm font-medium text-muted-foreground uppercase tracking-wider\">Temel Bilgiler</h3>\r\n                            <div className=\"grid grid-cols-1 md:grid-cols-3 gap-4\">\r\n                                <div className=\"space-y-2\">\r\n                                    <Label htmlFor=\"unit_category\">√únite T√ºr√º</Label>\r\n                                    <select\r\n                                        id=\"unit_category\"\r\n                                        name=\"unit_category\"\r\n                                        required\r\n                                        className=\"flex h-10 w-full rounded-md border border-input bg-background px-3 py-2 text-sm ring-offset-background focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring\"\r\n                                    >\r\n                                        <option value=\"\">Se√ßiniz...</option>\r\n                                        {UNIT_CATEGORIES.map(cat => (\r\n                                            <option key={cat} value={cat}>{cat}</option>\r\n                                        ))}\r\n                                    </select>\r\n                                </div>\r\n                                <div className=\"space-y-2\">\r\n                                    <Label htmlFor=\"type\">Oda Tipi</Label>\r\n                                    <select\r\n                                        id=\"type\"\r\n                                        name=\"type\"\r\n                                        required\r\n                                        className=\"flex h-10 w-full rounded-md border border-input bg-background px-3 py-2 text-sm ring-offset-background focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring\"\r\n                                    >\r\n                                        <option value=\"\">Se√ßiniz...</option>\r\n                                        <option value=\"1+1\">1+1</option>\r\n                                        <