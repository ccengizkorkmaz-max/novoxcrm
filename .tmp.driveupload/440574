l>\r\n                                    <Input name=\"description\" defaultValue={activity?.description || ''} placeholder=\"Detaylar...\" />\r\n                                </div>\r\n                            </>\r\n                        )}\r\n\r\n                        {isCompleteMode && (\r\n                            <>\r\n                                <div className=\"p-3 bg-muted rounded-md mb-2 text-sm\">\r\n                                    <span className=\"font-semibold\">{activity?.summary}</span> aktivitesini tamamlyorsunuz.\r\n                                </div>\r\n                                <div className=\"grid gap-2\">\r\n                                    <Label>Sonu (Outcome)</Label>\r\n                                    <select name=\"outcome\" className=\"flex h-10 w-full rounded-md border border-input bg-background px-3 py-2 text-sm\" required>\r\n                                        <option value=\"\">Seiniz</option>\r\n                                        <option value=\"Success\">Baarl / Olumlu</option>\r\n                                        <option value=\"Reached Interested\">Ulald - lgili</option>\r\n                                        <option value=\"Reached Not Interested\">Ulald - lgisiz</option>\r\n                                        <option value=\"No Answer\">Cevap Yok</option>\r\n                                        <option value=\"Busy\">Megul</option>\r\n                                        <option value=\"Follow Up Required\">Takip Gerekli</option>\r\n                                    </select>\r\n                                </div>\r\n                                <div className=\"grid gap-2\">\r\n                                    <Label>Grme Notlar</Label>\r\n                                    <Input name=\"notes\" placeholder=\"Grme nasl geti...\" required />\r\n                                </div>\r\n                                <div className=\"border-t pt-4 mt-